{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": ["html", "clear-text", "progress"],
  "testRunner": "jest",
  "coverageAnalysis": "all",
  "ignoreStatic": false,
  "dryRunTimeoutMinutes": 120,
  "concurrency": 4,
  "timeoutMS": 600000,
  "maxTestRunnerReuse": 50,
  "plugins": ["@stryker-mutator/jest-runner"],
  "mutate": [
    "src/utils/**/*.{ts,tsx}",
    "src/types/**/*.{ts,tsx}",
    "src/hooks/**/*.{ts,tsx}",
    "src/components/ExecutionStatusBadge.tsx",
    "src/components/LogLevelBadge.tsx",
    "src/components/editors/**/*.{ts,tsx}",
    "!src/**/*.test.{ts,tsx}",
    "!src/**/*.test.setup.ts",
    "!src/**/*.spec.{ts,tsx}",
    "!src/test/**",
    "!src/utils/errorFactory.ts"
  ],
  "checkers": [],
  "thresholds": {
    "high": 80,
    "low": 70,
    "break": 60
  },
  "jest": {
    "configFile": "./jest.config.cjs"
  },
  "logLevel": "info",
  "fileLogLevel": "trace"
}

